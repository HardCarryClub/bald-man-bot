{
  "name": "bald-man-bot",
  "version": "2.5.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run bot:generate && bun run start",
    "start": "bun run app/index.ts",
    "bot:generate": "bun run --bun dressed build -R bot",
    "bot:register-commands": "bun run --bun dressed build -r -R bot && bun run .dressed",
    "generate": "drizzle-kit generate",
    "migrate": "bun run --bun app/db/migrate.ts"
  },
  "dependencies": {
    "@dressed/ws": "0.4.4",
    "await-to-js": "3.0.0",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "discord-api-types": "0.38.23",
    "discord-fmt": "1.0.1",
    "dressed": "1.10.0-canary.5.7.3",
    "drizzle-orm": "0.44.5",
    "pino": "9.9.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@types/bun": "1.2.21",
    "drizzle-kit": "0.31.4",
    "lefthook": "1.12.4",
    "pino-pretty": "13.1.1"
  },
  "trustedDependencies": [
    "esbuild",
    "lefthook"
  ]
}
